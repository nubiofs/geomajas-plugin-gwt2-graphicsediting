<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:style>
		.toolbar {
			float: left;
		}
	</ui:style>

	<g:FlowPanel>
			<g:CheckBox ui:field="duplicateCheckbox" addStyleNames="{style.toolbar}">Duplicate On Drag</g:CheckBox>
			<g:ToggleButton ui:field="createLineButton" addStyleNames="{style.toolbar}">Line</g:ToggleButton>
			<g:ToggleButton ui:field="createPolygonButton" addStyleNames="{style.toolbar}">Polygon</g:ToggleButton>
			<g:ToggleButton ui:field="createRectangleButton" addStyleNames="{style.toolbar}">Rectangle</g:ToggleButton>
			<g:ToggleButton ui:field="createTextButton" addStyleNames="{style.toolbar}">Text</g:ToggleButton>
			<g:ToggleButton ui:field="createAnchoredTextButton" addStyleNames="{style.toolbar}">Anchored Text</g:ToggleButton>
			<g:ToggleButton ui:field="mapButton" addStyleNames="{style.toolbar}">Map</g:ToggleButton>
			<g:ToggleButton ui:field="createIconButton" addStyleNames="{style.toolbar}">Icon</g:ToggleButton>
			<g:ToggleButton ui:field="createAnchoredIconButton" addStyleNames="{style.toolbar}">Anchored Icon</g:ToggleButton>
	</g:FlowPanel>

</ui:UiBinder> 